<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="server.mapper.UserTypeMapper">
    <resultMap id="UserTypeDtoMap" type="UserTypeDto">
        <id property="id" column="id"/>
        <result property="type" column="type"/>
    </resultMap>

    <select id="getUserTypeDto" resultMap="UserTypeDtoMap">
        select *
        from UserType
        where id = #{id}
    </select>
</mapper>
